<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin">

    <TextView
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:textAppearance = "?android:attr/textAppearanceLarge"
        android:text = "Log In Successful"
        android:id = "@+id/textView6"
        android:layout_alignParentTop = "true"
        android:layout_centerHorizontal = "true"
        android:layout_margin = "16dp"/>

    <Button
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:text = "Log Out"
        android:id = "@+id/buttonThree"
        android:layout_centerVertical = "true"
        android:layout_centerHorizontal = "true"
        android:onClick="buttonLogout"/>

    <TextView
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:textAppearance = "?android:attr/textAppearanceLarge"
        android:text = "Welcome Back"
        android:id = "@+id/textView7"
        android:layout_below = "@+id/textView6"
        android:layout_centerHorizontal = "true"
        android:layout_margin = "16dp"/>
    <TextView
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:textAppearance = "?android:attr/textAppearanceMedium"
        android:text = "Medium Text"
        android:id = "@+id/textView8_username"
        android:layout_below = "@+id/textView7"
        android:layout_centerHorizontal = "true"
        android:layout_marginTop = "39dp"
        android:layout_margin = "16dp"/>
    <TextView
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:textAppearance = "?android:attr/textAppearanceMedium"
        android:text = "Medium Text"
        android:id = "@+id/textView8_userid"
        android:layout_marginTop = "36dp"
        android:layout_below = "@+id/textView8_username"
        android:layout_centerHorizontal = "true"
        android:layout_margin = "16dp"/>
    <Button
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:text = "Visa Status"
        android:id = "@+id/buttonVisa"
        android:layout_below = "@+id/buttonThree"
        android:layout_toLeftOf = "@+id/buttonThree"
        android:layout_marginTop = "16dp"
        android:layout_alignParentEnd = "false"
        android:onClick="buttonVisa"/>
    <Button
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:text = "Fees Check"
        android:id = "@+id/buttonFees"
        android:layout_alignTop = "@+id/buttonVisa"
        android:layout_alignParentRight = "true"
        android:layout_alignParentEnd = "false"
        android:layout_marginRight = "49dp"
        android:layout_marginEnd = "49dp"
        android:onClick="buttonFees"/>

</RelativeLayout>
